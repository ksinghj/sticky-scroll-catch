body {
  font-family: sans-serif;
}

.padding {
  padding: 50px 75px;
}

.border {
  border: 2px solid;
}

.flex {
  display: flex;
  flex-direction: row;
  padding: 0;
}

.left {
  padding-bottom: 700px;
  background: red;
}
.center {
  padding-bottom: 1200px;
  background: blue;
}
.right {
  padding-bottom: 900px;
  background: yellow;
}

/* target scrolled to bottom */
.sticky-scroll-catch--bottom {
  position: fixed;
  bottom: 0;
}

.sticky-scroll-catch--break {
  position: absolute;
  bottom: 0;
}

